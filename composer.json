{
    "name": "paneedesign/storage-bundle",
    "type": "symfony-bundle",
    "description": "Storage management for Symfony projects.",
    "keywords": ["storage", "aws", "file", "filesystem", "media", "gaufrette", "knp", "s3", "liip", "php"],
    "homepage": "https://www.paneedesign.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabiano Roberto",
            "email": "fabiano@paneedesign.com"
        },
        {
            "name": "Luigi Cardamone",
            "email": "luigi@paneedesign.com"
        },
        {
            "name":     "The contributors",
            "homepage": "https://github.com/paneedesign/storage-bundle/contributors"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-gd": "*",
        "gaufrette/extras": "~0.1",
        "knplabs/gaufrette": "~0.5",
        "knplabs/knp-gaufrette-bundle": "~0.5",
        "aws/aws-sdk-php": "^3.70",
        "liip/imagine-bundle": "~2.1",
        "fresh/doctrine-enum-bundle": "^6.2",
        "symfony/console": "4.2.*",
        "symfony/framework-bundle": "4.2.*",
        "symfony/twig-bundle": "4.2.*",
        "symfony/yaml": "4.2.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpmd/phpmd": "^2.6",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/filesystem": "4.2.*",
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "PaneeDesign\\StorageBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PaneeDesign\\StorageBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-flex": "feature/Symfony-flex"
        },
        "symfony": {
            "id": "01B246F85237FB1F3C796B2BF2B0DC6F",
            "allow-contrib": false,
            "require": "4.2.*"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
